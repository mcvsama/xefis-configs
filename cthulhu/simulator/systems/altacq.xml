<?xml version='1.0'?>
<xefis-include>
	<module name='systems/altacq'>
		<settings>
			<setting name='flag-altitude-difference.on' value='1000 ft'/>
			<setting name='flag-altitude-difference.off' value='100 ft'/>
		</settings>
		<properties>
			<!-- Input -->
			<property name='altitude.amsl' path='/systems/adc/altitude/amsl'/>
			<property name='altitude.acquire.amsl' path='/settings/cmd/altitude'/>
			<!-- Output -->
			<property name='altitude.acquire.flag' path='/systems/altacq/flag'/>
		</properties>
	</module>
</xefis-include>

