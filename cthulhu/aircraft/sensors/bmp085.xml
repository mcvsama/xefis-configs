<?xml version='1.0'?>
<xefis-include>
	<module name='io/bmp085'>
		<settings>
			<setting name='i2c.bus' value='10'/>
			<setting name='i2c.address' value='0x77'/>
			<setting name='pressure.read-interval' value='50 ms'/>
			<setting name='temperature.read-interval' value='500 ms'/>
		</settings>
		<properties path='/sensors'>
			<property name='serviceable' path='/pressure/serviceable'/>
			<property name='pressure' path='/pressure/static'/>
			<property name='temperature' path='/air-temperature/total'/>
		</properties>
	</module>
</xefis-include>

